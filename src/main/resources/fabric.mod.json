{
	"schemaVersion": 1,
	"id": "mccourse",
	"version": "${version}",
	"name": "MC Course",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"KofhWig"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mccourse/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.kofhwig.mccourse.MCCourse"
		],
		"fabric-datagen": [
			"me.kofhwig.mccourse.MCCourseDataGenerator"
		],
      "client": [
        "me.kofhwig.mccourse.MCCourseClient"
      ]
	},
	"mixins": [
		"mccourse.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}